FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 6.00e-2;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    
    outlet
    {
        type            zeroGradient;
    }

    wall
    {
        type            kLowReWallFunction;
        value           $internalField;
    }
    
    surface
    {
        type            kLowReWallFunction;
        value           $internalField;
    }
    
    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
