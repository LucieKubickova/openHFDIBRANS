startTime 	0;

stopAt          endTime;

endTime 	2000;

deltaT          1;

//~ writeControl    adjustableRunTime;

writeInterval 	$endTime;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

