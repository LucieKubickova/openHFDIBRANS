FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      HFDIBDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
surfaceThreshold    1e-4;
sdBasedLambda       true;
interfaceSpan       1.0;
saveIntInfo         false;

wallFunctions
{
	nut		nutkWallFunction;
	k		kLowReWallFunction;
	omega	omegaWallFunction;
	epsilon	epsilonWallFunction;
}

cutCellCorrection
{
    k               0.0;
    omega           1.0;
    epsilon         0.0;
    G               1.0;
    nut             0.0;
    ds              0.0;
}

