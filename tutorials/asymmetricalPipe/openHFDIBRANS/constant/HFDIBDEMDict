FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      HFDIBDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceThreshold    1e-4;
sdBasedLambda       true;
interfaceSpan       1.0;
saveIntInfo         false;
scaleDisG           true;
scaleCoeff          1.4142135623730951; // sqrt(2)

wallFunctions
{
	nut		nutkWallFunction;
	k		kLowReWallFunction;
	omega	omegaWallFunction;
	epsilon	epsilonWallFunction;
}

