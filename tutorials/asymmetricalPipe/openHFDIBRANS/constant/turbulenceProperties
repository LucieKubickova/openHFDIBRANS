FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      turbulenceProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

simulationType  HFDIBRAS;

HFDIBRAS
{
    HFDIBRASModel	    HFDIBKOmega;
    turbulence          on;
    printCoeffs         on;
    omegaSurfaceType    setValue;
    omegaBoundaryValue  1.0;
    kSurfaceType        setValue;
    kBoundaryValue      0.0;
    tolKEqn             1e-4;
    maxKEqnIters        10;
}

